{
	"increase_indent_pattern": "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*)$",
	"decrease_indent_pattern": "^((?!.*?\\/\\*).*\\*\\/)?\\s*[\\)\\}\\]].*$",
	"indent_next_line_pattern": "^\\s*\\w+\\s*\\(.*\\)\\s*(\\/\\/.*|\\/\\*.*\\*\\/\\s*)?$",
	"brackets": [
		{ "open": "\"", "close": "\"" },
		{ "open": "'", "close": "'" },
		{ "open": "(", "close": ")" },
		{ "open": "[", "close": "]" },
		{ "open": "{", "close": "}" }
	],
	"highlighters": [
		{
			"name": "root",
			"default_face": "default",
			"patterns": []
		}
	]
}
